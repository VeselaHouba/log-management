NGUSERNAME [a-zA-Z\.\@\-\+_%]+
NGUSER %{NGUSERNAME}
NGINX_TIMESTAMP %{MONTHDAY}/%{MONTH}/%{YEAR}:%{TIME}

NGINXACCESS %{IPORHOST:clientip} %{NGUSER:ident} %{NGUSER:auth} \[%{HTTPDATE:timestamp}\] "%{WORD:verb} %{URIPATHPARAM:request} HTTP/%{NUMBER:httpversion}" %{NUMBER:response}  (?:%{NUMBER:bytes}|-) (?:"(?:%{URI:referrer}|-)"|%{QS:referrer}) %{QS:agent} %{QS:xforwardedfor} %{IPORHOST:host} %{BASE10NUM:request_duration}
NGINXACCESS2 %{IPORHOST:clientip} (%{NGUSER:ident}|-) (%{NGUSER:auth}|-) \[%{NGINX_TIMESTAMP} %{DATA:timezone}\] "%{WORD:method} %{URIPATHPARAM:request} HTTP/%{NUMBER:httpversion}" %{NUMBER:response} (?:%{NUMBER:bytes}|-) (?:"(?:%{URI:referrer}|-)"|%{QS:referrer}) %{QS:agent} %{QS:xforwardedfor} Host=%{IPORHOST:nginx_host}
NGINXACCESS3 \[%{HTTPDATE:datetime}\] req="%{DATA:request}" st=%{NUMBER:status:int} len=%{NUMBER:lengt_bytes} ref="%{DATA:referer}" u_ag="%{DATA:useragent}" fwd_for="%{DATA:x_forwarded_for}" rmt=%{IPORHOST:remote_address} rrmt=%{DATA:realip_remote} cf=(-|%{IPORHOST:cf_connecting_ip}) up="%{DATA:upstream}" rt=%{NUMBER:request_time:float} uct="(-|%{NUMBER:upstream_connect_time:float}(, %{NUMBER:upstream_connect_time2:float})?)" uht="(-|%{NUMBER:upstream_header_time:float}(, %{NUMBER:upstream_header_time2:float})?)" urt="(-|%{NUMBER:upstream_response_time:float}(, %{NUMBER:upstream_response_time2:float})?)" req_host="%{DATA:vhost}"
